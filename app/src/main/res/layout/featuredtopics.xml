<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#FFFFFF"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/FeaturedTopicsPageID"
        android:layout_width="425dp"
        android:layout_height="81dp"
        android:background="#932424"
        android:text=" Featured Topics"
        android:textColor="#FFFFFF"
        android:textSize="45dp"
        tools:layout_editor_absoluteX="-4dp"
        tools:layout_editor_absoluteY="-2dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="134dp">

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputEditText
        android:layout_width="389dp"
        android:layout_height="60dp"
        android:hint="Search Topic"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="94dp" />

    <TextView
        android:id="@+id/bottomnavbar"
        android:layout_width="425dp"
        android:layout_height="62dp"
        android:background="#932424"
        android:text=""
        tools:layout_editor_absoluteX="-5dp"
        tools:layout_editor_absoluteY="579dp" />

    <ImageView
        android:id="@+id/pictureofcoco"
        android:layout_width="202dp"
        android:layout_height="173dp"
        app:srcCompat="@drawable/cocoimage"
        tools:layout_editor_absoluteX="-3dp"
        tools:layout_editor_absoluteY="154dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="104dp"
        android:layout_height="115dp"
        app:srcCompat="@drawable/socialmix"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="307dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="126dp"
        android:layout_height="117dp"
        app:srcCompat="@drawable/footballimage"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="439dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Code Coogs"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="217dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Social Mixer"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="352dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Football Game"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="491dp" />


</androidx.constraintlayout.widget.ConstraintLayout>